v3\\BankAPIv3\\frontend-vue\\src\\components\\accounts\\Index.vue"],"sourcesContent":["<template>\r\n  <section v-if=\"isAdmin\" class=\"table-accounts mx-4 p-4\">\r\n    <div class=\"container\">\r\n      <div class=\"button-container p-2 my-2\">\r\n        <button\r\n          @click=\"this.$router.push('/home')\"\r\n          class=\"py-2 mx-2 btn btn-danger\"\r\n        >\r\n          Go Back\r\n        </button>\r\n        <button\r\n          @click=\"this.$router.push('/addaccount')\"\r\n          class=\"py-2 mx-2 btn btn-primary\"\r\n        >\r\n          Add new Account\r\n        </button>\r\n      </div>\r\n      <table class=\"table align-middle mb-0 bg-white shadow-sm\">\r\n        <thead class=\"bg-light\">\r\n          <tr>\r\n            <th>IBan</th>\r\n            <th>AccountType</th>\r\n            <th>ActiveStatus</th>\r\n            <th>Balance</th>\r\n            <th>AbsLimit</th>\r\n            <th>OwnerID</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"account in accounts\" :key=\"account.id\" :account=\"account\">\r\n            <td>\r\n              <div class=\"d-flex align-items-center\">\r\n                <div class=\"ms-3\">\r\n                  <p class=\"text-muted mb-0\">{{ account.iban }}</p>\r\n                </div>\r\n              </div>\r\n            </td>\r\n            <td>\r\n              <p class=\"fw-normal mb-1\">{{ account.accountType }}</p>\r\n            </td>\r\n            <td>\r\n              <span\r\n                class=\"badge text-primary badge-success rounded-pill d-inline\"\r\n                >{{ account.active }}</span\r\n              >\r\n            </td>\r\n            <td>â‚¬{{ account.balance }}</td>\r\n            <td>â‚¬{{ account.absLimit }}</td>\r\n            <td>\r\n              {{ account.ownerId }}\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </section>\r\n</template>\r\n\r\n\r\n\r\n<script>\r\nimport axios from \"../../axios-auth\";\r\nimport { mapGetters } from \"vuex\";\r\nexport default {\r\n  name: \"Index\",\r\n  computed: {\r\n    ...mapGetters([\"isAdmin\"]),\r\n  },\r\n  setup() {},\r\n  data() {\r\n    return {\r\n      accounts: [],\r\n    };\r\n  },\r\n  mounted() {\r\n    let token = localStorage.getItem(\"token\");\r\n    axios\r\n      .request({\r\n        url: \"accounts\",\r\n        method: \"get\",\r\n        headers: {\r\n          Accept: \"application/json\",\r\n          \"Content-Type\": \"application/json\",\r\n          Authorization: `Bearer ${token}`,\r\n        },\r\n      })\r\n      .then((response) => {\r\n        this.accounts = response.data;\r\n      })\r\n      .catch((error) => {\r\n        console.log(error);\r\n        this.errored = true;\r\n      })\r\n      .finally(() => (this.loading = false));\r\n  },\r\n};\r\n</script>"]}]}