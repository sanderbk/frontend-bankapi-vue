"C:/Users/31624/Documents/GitHub/frontend-bankapi-vue/src/components/accounts/Index.vue","sourceRoot":"","sourcesContent":["<template>\r\n  <section class=\"table-accounts mx-4 p-4\">\r\n    <div class=\"container\">\r\n      <button\r\n        v-if=\"!isAdmin\"\r\n        @click=\"this.$router.push('/home')\"\r\n        class=\"py-2 mx-2 btn btn-danger\"\r\n      >\r\n        Go Back\r\n      </button>\r\n      <h4 v-if=\"!isAdmin\">Accounts</h4>\r\n      <div v-if=\"isAdmin\" class=\"form-control\">\r\n        <div class=\"input-group mb-3 w-70\">\r\n          <span class=\"input-group-text\">Search on firstname and lastname</span>\r\n          <input\r\n            type=\"text\"\r\n            v-model=\"firstnameInput\"\r\n            class=\"form-control\"\r\n            placeholder=\"Firstname...\"\r\n          />\r\n          <input\r\n            type=\"text\"\r\n            v-model=\"lastnameInput\"\r\n            class=\"form-control\"\r\n            placeholder=\"Lastname...\"\r\n          />\r\n          <button type=\"button\" class=\"btn btn-success\" @click=\"searchAccounts\">\r\n            Search for user\r\n          </button>\r\n        </div>\r\n      </div>\r\n      <table class=\"table align-middle mb-0 bg-white shadow-sm\">\r\n        <thead class=\"bg-light\">\r\n          <tr>\r\n            <th>IBan</th>\r\n            <th>AccountType</th>\r\n            <th>ActiveStatus</th>\r\n            <th>Balance</th>\r\n            <th>AbsLimit</th>\r\n            <th>Owner</th>\r\n            <th>Fname</th>\r\n            <th>Lname</th>\r\n            <th v-if=\"isAdmin\">Actions</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"account in accounts\" :key=\"account.id\" :account=\"account\">\r\n            <td>\r\n              <div class=\"d-flex align-items-center\">\r\n                <div class=\"ms-3\">\r\n                  <p class=\"text-muted mb-0\">{{ account.iban }}</p>\r\n                </div>\r\n              </div>\r\n            </td>\r\n            <td>\r\n              <p class=\"fw-normal mb-1\">{{ account.accountType }}</p>\r\n            </td>\r\n            <td>\r\n              <span class=\"badge text-primary badge-success rounded-pill d-inline\">{{\r\n                account.active\r\n              }}</span>\r\n            </td>\r\n            <td>€{{ account.balance }}</td>\r\n            <td>€{{ account.absLimit }}</td>\r\n            <td>\r\n              {{ account.username }}\r\n            </td>\r\n            <td>\r\n              {{ account.firstname }}\r\n            </td>\r\n            <td>\r\n              {{ account.lastname }}\r\n            </td>\r\n            <td v-if=\"isAdmin\">\r\n              <a class=\"btn btn-light\" @click=\"editAccount(account.iban)\">Edit ✏️</a>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n      <vue-awesome-paginate\r\n        v-if=\"isAdmin\"\r\n        class=\"w-100\"\r\n        v-model=\"currentPage\"\r\n        :total-items=\"totalElements\"\r\n        :items-per-page=\"size\"\r\n        :max-pages-shown=\"5\"\r\n        @click=\"handlePageChange\"\r\n      />\r\n      <div v-if=\"isAdmin\" class=\"form-control\">\r\n        <div class=\"button-container p-2 my-2 d-flex\">\r\n          <button @click=\"this.$router.push('/home')\" class=\"py-2 mx-2 btn btn-danger\">\r\n            Go Back\r\n          </button>\r\n          <button\r\n            @click=\"this.$router.push('/addaccount')\"\r\n            class=\"py-2 mx-2 btn btn-primary\"\r\n          >\r\n            Add new Account\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </section>\r\n  <!-- user settings -->\r\n  <section v-if=\"!isAdmin\" class=\"table-accounts mx-4 p-4\">\r\n    <div class=\"container\">\r\n      <div class=\"button-container p-2 my-2\"></div>\r\n      <h1 v-if=\"user\" class=\"text-center text-muted\">\r\n        Here is your userinfo: {{ user.username }}\r\n      </h1>\r\n      <table class=\"table align-middle mb-0 bg-white shadow-sm\">\r\n        <thead class=\"bg-light\">\r\n          <tr>\r\n            <th>Username</th>\r\n            <th>Fullname</th>\r\n            <th>Email</th>\r\n            <th>Phone</th>\r\n            <th>DayLimit</th>\r\n            <th>TransLimit</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody v-if=\"user\">\r\n          <tr>\r\n            <td>\r\n              <p class=\"fw-normal mb-1\">{{ user.username }}</p>\r\n            </td>\r\n            <td>\r\n              <span class=\"badge text-primary badge-success rounded-pill d-inline\">{{\r\n                user.firstname + \" \" + user.lastname\r\n              }}</span>\r\n            </td>\r\n            <td>{{ user.email }}</td>\r\n            <td>{{ user.phone }}</td>\r\n            <td>{{ user.dayLimit }}</td>\r\n            <td>{{ user.transLimit }}</td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"../../axios-auth\";\r\nimport { mapGetters } from \"vuex\";\r\n\r\nexport default {\r\n  name: \"Index\",\r\n  computed: {\r\n    ...mapGetters([\"isAdmin\"]),\r\n    ...mapGetters([\"getUserName\"]),\r\n  },\r\n  setup() {},\r\n  data() {\r\n    return {\r\n      currentPage: 1,\r\n      size: 5,\r\n      accounts: [],\r\n      user: null,\r\n      totalPages: 0,\r\n      totalElements: 0,\r\n      setUsername: \"\",\r\n      setType: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    editAccount(iban) {\r\n      this.$router.push({ name: \"EditAccount\", params: { iban } });\r\n    },\r\n    handlePageChange(newPage) {\r\n      this.currentPage = newPage;\r\n      console.log(newPage);\r\n      this.searchAccounts();\r\n    },\r\n    searchUser() {\r\n      console.log(this.getUserName);\r\n\r\n      axios\r\n        .get(\"users/username/\" + this.getUserName)\r\n        .then((res) => {\r\n          this.user = res.data;\r\n\r\n          this.fetchedUser = JSON.stringify(res.data);\r\n          this.errMsg = \"\";\r\n          console.log(\"fefeef: \" + this.fetchedUser);\r\n          // this.disable = false;\r\n        })\r\n        .catch((error) => {\r\n          // Use arrow function here\r\n          this.errMsg = \"User not found\";\r\n          console.log(\"gaat niet goed\");\r\n          console.log(error);\r\n        });\r\n    },\r\n    searchAccounts() {\r\n      let token = localStorage.getItem(\"token\");\r\n\r\n      // Create the headers object\r\n      let headers = {\r\n        Accept: \"application/json\",\r\n        \"Content-Type\": \"application/json\",\r\n        Authorization: `Bearer ${token}`,\r\n      };\r\n\r\n      if (!this.isAdmin) {\r\n        this.setUsername = this.getUserName;\r\n      }\r\n\r\n      // Create the params object\r\n      let params = {\r\n        firstname: this.firstnameInput,\r\n        lastname: this.lastnameInput,\r\n        username: this.setUsername,\r\n        iban: \"\",\r\n        size: this.size,\r\n        page: this.currentPage - 1,\r\n      };\r\n\r\n      axios\r\n        .get(\"accounts/search\", {\r\n          params: params,\r\n          headers: headers,\r\n        })\r\n        .then((response) => {\r\n          this.totalPages = response.data.totalPages;\r\n          this.totalElements = response.data.totalElements;\r\n\r\n          console.log(response.data.totalElements);\r\n\r\n          console.log(response.data.totalPages);\r\n\r\n          console.log(response.data.pageable);\r\n\r\n          this.accounts = response.data.content;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n          this.errored = true;\r\n        })\r\n        .finally(() => (this.loading = false));\r\n    },\r\n  },\r\n  mounted() {\r\n    this.searchAccounts();\r\n    this.searchUser();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.pagination-container {\r\n  background-color: #f0f0f0;\r\n  border-radius: 5px;\r\n  padding: 10px 0px;\r\n}\r\n.paginate-buttons {\r\n  width: 40px;\r\n  height: 40px;\r\n  margin-inline: 5px;\r\n  cursor: pointer;\r\n  border: none;\r\n  background-color: transparent;\r\n  border-radius: 2px;\r\n}\r\n.back-button {\r\n  width: 70px;\r\n}\r\n.next-button {\r\n  width: 70px;\r\n}\r\n.back-button svg {\r\n  transform: rotate(180deg);\r\n}\r\n.active-page {\r\n  background-color: #2980b9;\r\n  color: #fff;\r\n}\r\n\r\n.paginate-buttons:hover {\r\n  background-color: #e5e5e5;\r\n}\r\n.active-page:hover {\r\n  background-color: #3b8cc3;\r\n  color: #fff;\r\n}\r\n.back-button:active,\r\n.next-button:active {\r\n  background-color: #dedede;\r\n}\r\n</style>\r\n"]}]}