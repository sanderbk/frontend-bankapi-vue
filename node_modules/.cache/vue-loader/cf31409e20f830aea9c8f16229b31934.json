his.dob,\r\n        address: this.address,\r\n        email: this.email,\r\n        phone: this.phone,\r\n        registeredOn: today.toJSON(),\r\n        dayLimit: parseFloat(this.dayLimit),\r\n        transLimit: parseFloat(this.transLimit),\r\n        active: true,\r\n      });\r\n      axios\r\n        .post(\"users\", data, config)\r\n        .then((response) => {\r\n          this.loading = false; // Set loading to false\r\n          this.$router.replace(\"/login\");\r\n          console.log(response);\r\n        })\r\n        .catch((error) => {\r\n          this.errorms = error.response.data.reason;\r\n          this.loading = false; // Set loading to false\r\n          console.log(error);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.vue-butt {\r\n  display: inline-block;\r\n}\r\n.ftco-section {\r\n  height: 100%;\r\n  background: radial-gradient(circle, rgb(238, 238, 238) 0%, rgb(233, 233, 233));\r\n}\r\n</style>\r\n"]}]}